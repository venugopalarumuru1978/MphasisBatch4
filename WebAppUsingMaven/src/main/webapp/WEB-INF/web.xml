<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddStudent</servlet-name>
  	<display-name>AddStudent</display-name>
  	<description></description>
  	<servlet-class>com.servlets.apps.AddStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewStudents</servlet-name>
  	<display-name>ViewStudents</display-name>
  	<description></description>
  	<servlet-class>com.servlets.apps.ViewStudents</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddStudent</servlet-name>
  	<url-pattern>/AddStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewStudents</servlet-name>
  	<url-pattern>/ViewStudents</url-pattern>
  </servlet-mapping>
</web-app>

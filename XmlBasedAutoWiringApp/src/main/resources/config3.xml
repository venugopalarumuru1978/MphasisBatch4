<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="adrsObj1"  class="com.beans.Address">
		<property name="dno" value="D.No. 1-121-A"/>
		<property name="loc" value="Hyderabad"/>
		<property name="phone" value="9000012345"/>
	</bean>

	<bean id="adrsObj2"  class="com.beans.Address">
		<property name="dno" value="D.No. 1-122-A"/>
		<property name="loc" value="Hyderabad"/>
		<property name="phone" value="9000012341"/>
	</bean>
	
	<bean id="empObj"  class="com.beans.Employee">
		<property name="empno" value="1001"/>
		<property name="ename" value="Praveen Kumar"/>
	</bean>
	
</beans>